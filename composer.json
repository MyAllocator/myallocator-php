{
    "name": "myallocator/myallocator-php-sdk",
    "type": "library",
    "description": "Myallocator PHP SDK",
    "license": "MIT",
    "keywords": ["myallocator", "channel", "manager"],
    "homepage": "https://www.myallocator.com/",
    "authors": [
        {
            "name": "Nathan Helenihi",
            "email": "nathan.helenihi@cloudbeds.com",
            "role": "Developer"
        },
        {
            "name": "Sterling Phillips",
            "email": "sterling.phillips@cloudbeds.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*"
    },
    "autoload": {
        "psr-4": {
            "MyAllocator\\phpsdk\\src\\": "src/MyAllocator",
            "MyAllocator\\phpsdk\\tests\\json\\": "tests/json",
            "MyAllocator\\phpsdk\\tests\\xml\\": "tests/xml"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    }
}
